{
  "name": "mern-boilerplate",
  "version": "0.5.0",
  "description": "A full MERN stack boilerplate",
  "main": "server/index.js",
  "scripts": {
    "build": "./scripts/build-prod",
    "build:prod": "./scripts/build-prod",
    "build:dev": "./scripts/build-dev",
    "start": "nodemon server/index.js",
    "dev": "./scripts/watch-dev",
    "prod": "npm run build && npm run start",
    "lint": "./scripts/lint",
    "lint:fix": "./scripts/lint-fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:verbose": "jest --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djizco/mern-boilerplate.git"
  },
  "author": {
    "name": "Rico Rojas",
    "email": "djizco@yahoo.com",
    "url": "https://github.com/djizco"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djizco/mern-boilerplate/issues"
  },
  "keywords": [
    "react",
    "redux",
    "webpack",
    "express",
    "passport",
    "bulma",
    "fontawesome",
    "ramda",
    "eslint",
    "jest",
    "enzyme"
  ],
  "homepage": "https://github.com/djizco/mern-boilerplate#readme",
  "engines": {
    "node": ">=10.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "client/**/*.{js,jsx}",
      "server/**/*.{js}"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "^_atoms(.*)$": "<rootDir>/client/components/atoms/$1",
      "^_molecules(.*)$": "<rootDir>/client/components/molecules/$1",
      "^_organisms(.*)$": "<rootDir>/client/components/organisms/$1",
      "^_templates(.*)$": "<rootDir>/client/components/templates/$1",
      "^_pages(.*)$": "<rootDir>/client/components/pages/$1",
      "^_environment(.*)$": "<rootDir>/client/components/environment/$1",
      "^_hooks(.*)$": "<rootDir>/client/hooks/$1",
      "^_store(.*)$": "<rootDir>/client/store/$1",
      "^_actions(.*)$": "<rootDir>/client/store/actions/$1",
      "^_thunks(.*)$": "<rootDir>/client/store/thunks/$1",
      "^_reducers(.*)$": "<rootDir>/client/store/reducers/$1",
      "^_assets(.*)$": "<rootDir>/client/assets/$1",
      "^_styles(.*)$": "<rootDir>/client/styles/$1",
      "^_utils(.*)$": "<rootDir>/client/utils/$1",
      "^_api(.*)$": "<rootDir>/client/api/$1"
    },
    "setupFiles": [
      "<rootDir>/config/jest-setup.js",
      "<rootDir>/config/enzyme-setup.js"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.21",
    "@fortawesome/free-brands-svg-icons": "^5.10.1",
    "@fortawesome/free-regular-svg-icons": "^5.10.1",
    "@fortawesome/free-solid-svg-icons": "^5.10.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "clean-webpack-plugin": "^3.0.0",
    "connect-mongo": "^3.0.0",
    "connected-react-router": "^6.5.2",
    "core-js": "^3.2.1",
    "date-fns": "^1.30.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "history": "^4.9.0",
    "immutability-helper": "^3.0.1",
    "json-style-converter": "^1.0.3",
    "mongoose": "^5.6.9",
    "mongoose-auto-increment-reworked": "^1.2.1",
    "mongoose-immutable": "0.0.2",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "prop-types": "^15.7.2",
    "ramda": "^0.26.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-notification-system": "^0.2.17",
    "react-notification-system-redux": "^2.0.0",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "uuid": "^3.3.2",
    "webpack": "^4.39.2",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "bulma": "0.7.5",
    "classnames": "^2.2.6",
    "css-loader": "^3.2.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "favicons-webpack-plugin": "1.0.1",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^5.0.0",
    "jest": "^24.9.0",
    "jest-environment-jsdom": "^24.9.0",
    "jest-environment-jsdom-global": "^1.2.0",
    "less": "3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "sass-loader": "^7.2.0",
    "style-loader": "^1.0.0",
    "superagent": "^5.1.0",
    "supertest": "^4.0.2",
    "url-loader": "^2.1.0"
  }
}
